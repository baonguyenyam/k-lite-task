
- var menu = [{'Name':'Home', 'URL':'/index.html', 'Icon':'home_icon.png'}, {'Name':'Contact', 'URL':'/contact', 'Icon':'contact_icon.png'}];

<!-- ////////////// HEADER ////////////// -->
header.pos-f-t
	<!-- ////////////// GLOBAL MENU ////////////// -->
	nav.navbar.navbar-toggleable-md.navbar-light.bg-faded
		.container
			button.navbar-toggler.navbar-toggler-right(type='button', data-toggle='collapse', data-target='#VienSoiGlobalMenu', aria-controls='VienSoiGlobalMenu', aria-expanded='false', aria-label='Toggle navigation')
				span.navbar-toggler-icon
			a.navbar-brand.hidden-lg-up(href='/index.html')
				strong NAME
			#VienSoiGlobalMenu.collapse.navbar-collapse
				ul#VienSoiNAV.navbar-nav.mr-auto
					each val, key in menu
						if (typeof(val) == "object")
							if selected === val.Name
								li.nav-item.active
									a.nav-link.align-items-center(href=val.URL, title=val.Name)
										img.img-fluid(src='/images/'+val.Icon, alt=val.Name)
										span=val.Name
							else
								li.nav-item
									a.nav-link.align-items-center(href=val.URL, title=val.Name)
										img.img-fluid(src='/images/'+val.Icon, alt=val.Name)
										span=val.Name
				.navbar-text Right Content
<!-- ////////////// HEADER ////////////// -->
